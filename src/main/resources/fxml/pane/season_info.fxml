<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Text?>
<fx:root xmlns:fx="http://javafx.com/fxml" type="javafx.scene.layout.AnchorPane" xmlns="http://javafx.com/javafx">
    <ImageView fx:id="image" fitHeight="260" fitWidth="175" pickOnBounds="true" AnchorPane.leftAnchor="20" AnchorPane.topAnchor="20" />
    <VBox spacing="5" AnchorPane.leftAnchor="210.0" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="20.0">
        <Text id="title" fx:id="title" wrappingWidth="550" />
        <Text id="description" fx:id="description" wrappingWidth="550" />
    </VBox>
</fx:root>
